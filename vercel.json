{
"version": 2,
"framework": null,
"functions": {
"api/index.php": {
"runtime": "vercel-php@0.7.5"
}
},
"routes": [
{
"src": "/build/(.)",
"dest": "/public/build/$1"
},
{
"src": "/(.)",
"dest": "/api/index.php"
}
],
"env": {
"APP_ENV": "production",
"APP_DEBUG": "false",
"APP_URL": "https://www.google.com/search?q=https://your-domain.vercel.app",
"APP_KEY": "base64:YOUR_GENERATED_KEY"
}
}